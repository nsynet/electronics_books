0000817E                    	.CODE
                            	.PUBLIC		_SP_SIOInitial;
                            	_SP_SIOInitial:	.PROC
                            	
                            			 	//系统时钟设定
0000817E 09 93 98 00        			 	r1 = 0x0098;    		 		//Fosc=49mHz 
00008180 19 D3 13 70        			 	[P_SystemClock] = r1;
                            		 	
00008182 11 93 07 70        				r1 = [P_IOB_Dir];
00008184 41 A2              				r1 |= 1;						//set bit0 output for sck
00008185 19 D3 07 70        				[P_IOB_Dir] = r1;
                            				
00008187 11 93 08 70        				r1 = [P_IOB_Attrib];
00008189 41 A2              				r1 |= 1;						//buffer output
0000818A 19 D3 08 70        				[P_IOB_Attrib] = r1;
                            				
0000818C 11 93 06 70        				r1 = [P_IOB_Buffer];
0000818E 41 A2              				r1 |= 1;						//output high
0000818F 19 D3 05 70        				[P_IOB_Data] = r1;
00008191 90 9A              			 	retf
                            	.ENDP
                            	//////////////////////////////////////////////////////////////////
                            	// Function: Send A Byte to Serial Flash
                            	// Syntax: SP_SIOSendAByte(AddressLow,AddressHigh,  data)
                            	// c level public
                            	// Used register: r1,r2,r3
                            	//////////////////////////////////////////////////////////////////
                            	.public _SP_SIOSendAByte;
                            	_SP_SIOSendAByte: .PROC
                            	F_SIOSendAByte:
00008192 88 DA              		    PUSH BP,BP TO [SP];
00008193 08 0B 01 00        		    BP = SP + 1;
00008195 03 92              			R1 = [BP+3];
00008196 19 D3 1B 70        		    [P_SIO_Addr_Low]=r1;        // input SFLASH low address
00008198 79 93              		    r1=r1 lsr 4;         		// right shift 8
00008199 79 93              		    r1=r1 lsr 4;
0000819A 19 D3 1C 70        		    [P_SIO_Addr_Mid]=r1; 		// input SFLASH mid address
0000819C 04 92              		    R1 = [BP+4];                // Port direction
0000819D 47 B2              		    r1=r1&0x0007;  				// input SFLASH hi address
0000819E 19 D3 1D 70        		    [P_SIO_Addr_High]=r1;
000081A0 09 93 D3 00        	        r1=0x00C3+C_SIOCLOCK;
000081A2 19 D3 1E 70        	        [P_SIO_Ctrl]=r1;        	// clk=CPUclk/8, 24 bit address  ;write
000081A4 19 D3 1F 70        	        [P_SIO_Start]=r1;       	// enable write mode
000081A6 05 92              	        R1 = [BP+5];
000081A7 19 D3 1A 70        	        [P_SIO_Data]=r1;        	// state to transmit data
                            	L_WaitSIOSendReady:
000081A9 11 93 1F 70        	        r1=[P_SIO_Start];
000081AB 09 C3 80 00        	        test    r1,0x0080
000081AD 45 4E              	        jnz     L_WaitSIOSendReady
000081AE 19 D3 20 70        	        [P_SIO_Stop]=r1;            //disable write mode
000081B0 40 F0 93 82        	        call  F_Delay_Program_Time
000081B2 88 98              	        POP BP,BP FROM [SP];
000081B3 90 9A              	        retf;
                            	        .ENDP;
                            	
                            	
