0000809A 88 D2              	                PUSH r1,r1 TO [sp];
0000809B 19 D3 11 70        	     			[P_INT_Clear] = r1;
0000809D 88 90              	                POP r1,r1 FROM [sp];
0000809E 98 9A              	                reti;
                            	
                            	_IRQ1:        
0000809F 88 D2              	                PUSH r1,r1 TO [sp];
000080A0 19 D3 11 70        	                [P_INT_Clear] = r1;
000080A2 88 90              	                POP r1,r1 FROM [sp];
000080A3 98 9A              	                reti;
