                            	/////////////////////////////////////////////////////////////////////////////////////////        
                            	_BREAK:        
00008095 88 D2              	                PUSH r1,r1 TO [sp];
00008096 19 D3 11 70        					[P_INT_Clear] = r1;                
00008098 88 90              	                POP r1,r1 FROM [sp];
00008099 98 9A              	                reti;
                            	
