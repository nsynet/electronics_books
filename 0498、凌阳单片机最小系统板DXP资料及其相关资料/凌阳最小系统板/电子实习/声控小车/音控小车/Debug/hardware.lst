                            	///////////////////////////////////////////
                            	// Function: Initial Queue
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	_SP_InitQueue:	.PROC
                            	_SP_InitQueue_A2000:
                            	_SP_InitQueue_S480:
                            	_SP_InitQueue_S240:
                            	_SP_InitQueue_MS01:
                            	_SP_InitQueue_DVR:
                            	
                            	F_SP_InitQueue_A2000:
                            	F_SP_InitQueue_S480:
                            	F_SP_InitQueue_S240:
                            	F_SP_InitQueue_MS01:
                            	F_SP_InitQueue_DVR:
                            	F_SP_InitQueue:
0000CB3F 09 93 8B 04        			R1 = R_Queue
0000CB41 40 94              			R2 = 0 
                            	L_ClearQueueLoop?:		
0000CB42 D1 D4              			[R1++] = R2
0000CB43 09 43 EF 04        			cmp	R1, R_Queue+C_QueueSize
0000CB45 44 4E              			jne	L_ClearQueueLoop?
                            				
0000CB46 40 92              			R1 = 0
0000CB47 19 D3 EF 04        			[R_ReadIndex] = R1
0000CB49 19 D3 F0 04        			[R_WriteIndex] = R1
                            					
0000CB4B 90 9A              			RETF
                            			.ENDP
                            			
                            	///////////////////////////////////////////
                            	// Function: Get a data form Queue
                            	// Output:  R1: Data
                            	//			R2: return value
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_ReadQueue_A2000:
                            	F_SP_ReadQueue_S480:
                            	F_SP_ReadQueue_S240:
                            	F_SP_ReadQueue_MS01:
                            	F_SP_ReadQueue_DVR:
                            	F_SP_ReadQueue:
0000CB4C 12 95 EF 04        			R2 = [R_ReadIndex]
0000CB4E 12 45 F0 04        			cmp R2,[R_WriteIndex]
0000CB50 0D 5E              			je	L_RQ_QueueEmpty
                            	
0000CB51 0A 05 8B 04        			R2 += R_Queue				// get queue data address
0000CB53 C2 92              			R1 = [R2]
                            			
0000CB54 12 95 EF 04        			R2 = [R_ReadIndex]
0000CB56 41 04              			R2 += 1
0000CB57 0A 45 64 00        			cmp	R2, C_QueueSize
0000CB59 01 4E              			jne	L_RQ_NotQueueBottom
0000CB5A 40 94              			R2 = 0
                            	L_RQ_NotQueueBottom:	
0000CB5B 1A D5 EF 04        			[R_ReadIndex] = R2		
                            			//r2 = 0x0000 						// get queue data
0000CB5D 90 9A              			retf 
                            	L_RQ_QueueEmpty:
                            			//r2 = 0x8000 						// queue empty
0000CB5E 90 9A              			retf 
                            	
                            	///////////////////////////////////////////
                            	// Function: Get a data from Queue but do 
                            	//			not change queue index
                            	// R1: output
                            	// Destory: R1,R2
                            	///////////////////////////////////////////
                            	F_SP_ReadQueue_NIC:
                            	F_SP_ReadQueue_NIC_A2000:
                            	F_SP_ReadQueue_NIC_S480:
                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
0000CB5F 12 95 EF 04        			R2 = [R_ReadIndex]
0000CB61 12 45 F0 04        			cmp R2,[R_WriteIndex]
0000CB63 03 5E              			je	L_RQ_QueueEmpty?
                            	
0000CB64 0A 05 8B 04        			R2 += R_Queue				// get queue data index
0000CB66 C2 92              			R1 = [R2]
                            	L_RQ_QueueEmpty?:
0000CB67 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_WriteQueue_A2000:
                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
0000CB68 12 95 F0 04        			R2 = [R_WriteIndex] 			// put data to queue
0000CB6A 0A 05 8B 04        			R2 += R_Queue
0000CB6C C2 D2              			[R2] = R1
                            	
0000CB6D 12 95 F0 04        			R2 = [R_WriteIndex]
0000CB6F 41 04              			R2 += 1
0000CB70 0A 45 64 00        			cmp	R2, C_QueueSize
0000CB72 01 4E              			jne	L_WQ_NotQueueBottom
0000CB73 40 94              			R2 = 0
                            	L_WQ_NotQueueBottom:
0000CB74 1A D5 F0 04        			[R_WriteIndex] = R2
0000CB76 90 9A              			RETF
                            	
                            			
