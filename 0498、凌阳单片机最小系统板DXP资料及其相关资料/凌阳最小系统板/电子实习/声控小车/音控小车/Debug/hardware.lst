                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000CD08 88 DA              			push bp to [sp] 
0000CD09 08 0B 01 00        			bp = sp + 1 
                            			
0000CD0B 03 92              			r1 = [bp+3] 					// Address
0000CD0C 04 94              			r2 = [bp+4] 					// Page
                            			
0000CD0D 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
