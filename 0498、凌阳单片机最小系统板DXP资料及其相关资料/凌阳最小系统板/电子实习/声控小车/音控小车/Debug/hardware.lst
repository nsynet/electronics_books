                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000CC65 90 D4              			push r1,r2 to [sp] 
0000CC66 11 93 17 70        	        r1=[P_DAC1] 
0000CC68 09 B3 C0 FF        	        r1 &= ~0x003f 
0000CC6A 09 43 00 80        	        cmp     r1,0x8000
0000CC6C 0E 0E              	        jb     	L_RU_NormalUp
0000CC6D 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000CC6E 40 F0 D1 CC        	        call    F_Delay         
0000CC70 41 94              	        r2 = 0x0001 
0000CC71 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000CC73 09 23 40 00        	        r1 -= 0x40 
0000CC75 19 D3 17 70        	        [P_DAC1] = r1 
0000CC77 09 43 00 80        	        cmp     r1,0x8000 
0000CC79 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000CC7A 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000CC7B 40 F0 D1 CC        	        call    F_Delay 
0000CC7D 41 94              	        r2 = 0x0001 
0000CC7E 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000CC80 09 03 40 00        	        r1 += 0x40 
0000CC82 19 D3 17 70        	        [P_DAC1] = r1 
0000CC84 09 43 00 80        	        cmp     r1, 0x8000 
0000CC86 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000CC87 90 90              			pop     r1,r2 from [sp] 
0000CC88 90 9A              	  		retf 
                            	    	.ENDP
                            	    
