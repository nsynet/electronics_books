0000CB77 11 93 EF 04        			R1 = [R_ReadIndex] 
0000CB79 11 43 F0 04        			cmp R1,[R_WriteIndex] 
0000CB7B 12 5E              			je	L_TQ_QueueEmpty 
                            	
                            			//... Test Queue Full ...
0000CB7C 11 93 EF 04        			R1 = [R_ReadIndex] 				// For N Queue Full: 1.R=0 and W=N-1 2. R<>0 and W=R-1 
0000CB7E 05 4E              			jnz	L_TQ_JudgeCond2 
0000CB7F 11 93 F0 04        			R1 = [R_WriteIndex] 
0000CB81 09 43 63 00        			cmp	R1, C_QueueSize-1 			// Cond1
0000CB83 08 5E              			je	L_TQ_QueueFull 			
                            	L_TQ_JudgeCond2:		
0000CB84 11 93 EF 04        			R1 = [R_ReadIndex] 
0000CB86 41 22              			R1 -=1 
0000CB87 11 43 F0 04        			cmp R1,[R_WriteIndex] 
0000CB89 02 5E              			je	L_TQ_QueueFull 	
                            	
0000CB8A 40 92              			r1 = 0 							// not Full, not empty
0000CB8B 90 9A              			retf 
                            	L_TQ_QueueFull:
0000CB8C 41 92              			r1 = 1 							// full
0000CB8D 90 9A              			retf 
                            	L_TQ_QueueEmpty:
0000CB8E 42 92              			r1 = 2 							// empty
0000CB8F 90 9A              			retf 
                            	
                            	
                            	
                            	
                            	
