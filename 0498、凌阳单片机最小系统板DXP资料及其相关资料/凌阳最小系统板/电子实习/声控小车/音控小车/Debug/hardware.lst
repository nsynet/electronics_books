0000CD0F 4A 95              			r2 = r2 lsl 2 
                            			
0000CD10 7F BC              			sr &= 0x03f 					// Change Page
0000CD11 06 A5              	        r2 |=sr 						//
0000CD12 02 9D              	        sr = r2 						//
                            	  
0000CD13 E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000CD14 88 98              			pop	 bp from [sp] 
0000CD15 90 9A              			retf 
                            			.ENDP 
                            	//........................................
                            	F_SP_GetResource:
0000CD16 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
